{
    "files.watcherExclude": {
        "**/OUTPUT/**": true
    },
    "files.exclude": {
        "**/OUTPUT": true,
        "**/*.out": true
    },
    "C_Cpp.default.includePath": [
        "OUTPUT/include/"
    ],
    "files.associations": {
        "iostream": "cpp",
        "*.tcc": "cpp",
        "memory": "cpp",
        "functional": "cpp"
    }
}